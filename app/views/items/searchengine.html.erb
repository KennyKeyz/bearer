




<p id="notice"><%= notice %></p>

<h1>Items</h1>
<% if @items.empty? %>

       <div class="card mb-3">
        <div class="card-header">
          <i class="fa fa-table"></i> Items </div>
        <div class="card-body">
                    <h1> Not found </h1>

                  </div>
                  
                  </div>

                  <% else %>

                   <div class="card mb-3">
        <div class="card-header">
          <i class="fa fa-table"></i> Items Table </div>
        <div class="card-body">
          <div class="table-responsive">
            <table class="table table-striped table-bordered dt-responsive nowrap" id="activitiesTable" width="100%" cellspacing="0">
              <thead>
    <tr>
      <th>Name</th>
      <th>Status</th>
      <th>Tracking number</th>
      <th>Code</th>
    
    </tr>
  </thead>

           <tfoot>
                <tr>
               <th>Name</th>
      <th>Status</th>
      <th>Tracking number</th>
      <th>Code</th>

                
                </tr>
              </tfoot>

  <tbody>
    <% @items.each do |item| %>
      <tr>
        <td><%= item.name %></td>
        <td><%= item.status %></td>
        <td><%= item.tracking_number %></td>
        <td><%= item.code %></td>
    
      </tr>
    <% end %>
  </tbody>
</table>

<br>
</div>
</div>
</div>


<%= link_to 'New Item', new_item_path %>



<% end %>





